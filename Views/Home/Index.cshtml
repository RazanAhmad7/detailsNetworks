@{
    ViewData["Title"] = "Home Page";
    Layout = null;
}
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DetailsNetworks</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/DetailsNetworks.styles.css" asp-append-version="true" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.3.0/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" rel="stylesheet">
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <style>
        .toast {
            background: rgba(255, 255, 255, 0.05);
            border-left: 5px solid var(--main-orange);
            font-size: 1rem;
        }

        .contact-forms-container {
    display: flex;
    gap: 2rem;
    align-items: flex-start;
}

.contact-form-left,
.contact-form-right {
    flex: 1;
}

.form-wrapper {
    background-color: rgba(9, 6, 80, 0.1);
    border: 1px solid white;
    border-radius: 1rem;
    padding: 2rem;
}

.form-title {
    font-size: 1.8rem;
    color: white;
    margin-bottom: 1.5rem;
    text-align: center;
}

/* Star rating styles */
#starRating i {
    color: rgba(255, 255, 255, 0.3);
    cursor: pointer;
    transition: color 0.2s ease;
    font-size: 3rem;
}

#starRating i.active,
#starRating i:hover {
    color: var(--main-orange);
}

/* Mobile responsiveness */
@@media (max-width: 768px) {
    .contact-forms-container {
        flex-direction: column;
        gap: 1.5rem;
    }
    
    .form-title {
        font-size: 1.5rem;
    }
}
    </style>
</head>

    <body>

    <!-- Hero Section -->
    <section class="hero-section gradient-bg">
        <!-- Top contact info bar -->
        <div class="container-fluid py-1 px-3 d-none d-lg-flex justify-content-end">
            <div class="me-3" style="color:#fff; font-size:1rem; font-weight:600;">
                079 009 0914
            </div>
            <div class="me-2" style="color:#fff; font-size:0.95rem;">
                ibrahim@detailsnetworks.com
            </div>
        </div>

        <nav class="navbar p-0 navbar-expand-lg navbar-dark mt-2 mx-3 mb-4"
             style=" background: rgba(9,6,80,0.95); border-radius: 2rem;border: 2px solid #fff;">
            <div class="container-fluid align-items-center">
                <a class="navbar-brand d-flex align-items-center" href="#">
                    <div style="width:80px; height:65px; margin-left:15px; border-radius:50%; display:flex; align-items:center; justify-content:center; margin-right:10px;">
                       <img src="~/images/الشعار png.png" width="160" height="135" alt="Alternate Text" />
                    </div>
                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#mainNavbar"
                        aria-controls="mainNavbar" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse justify-content-center" id="mainNavbar">
                    <ul class="navbar-nav mb-lg-0">
                        <li class="nav-item mx-2">
                            <a class="nav-link text-white fw-bold" href="#about">About Details Networks</a>
                        </li>
                        <li class="nav-item mx-2">
                            <a class="nav-link text-white fw-bold" href="#services">Installations & Solutions</a>
                        </li>
                        <li class="nav-item mx-2">
                            <a class="nav-link text-white fw-bold" href="#it">Managed IT Services</a>
                        </li>
                        <li class="nav-item mx-2">
                            <a class="nav-link text-white fw-bold" href="#support">Remote Support</a>
                        </li>
                    </ul>
                </div>
                <div class="d-flex align-items-center ms-auto">
                   
                    <a href="#contact" class="btn btn-outline-light rounded-pill px-4 py-2 fw-bold"
                       style="border-width:2px; font-size:1.1rem; background:#EB5336 ;">Contact Us</a>
                </div>
            </div>
        </nav>
        <div class="container">
            <h1 class="hero-title mb-3">
                Smart Installs. Stronger Connections.<br><span class="accent">
                    IT's that
                    Simple.
                </span>
            </h1>
            <p class="hero-desc mb-4">
                MTC Networks specializes in structured cabling, WiFi solutions, access control,
                security camera installations and audio systems. MTC Networks provides high-quality tech solutions designed
                for seamless integration and optimal performance for <span style="color:#EB5336;">commercial</span> and
                <span style="color:#EB5336;">residential</span> clients.
            </p>
            <div class="hero-icons mb-4">
                <a href="#faqs" class="hero-icon-link"><span class="me-1">&#x2753;</span> FAQs</a>
                <a href="#why" class="hero-icon-link"><span class="me-1">&#x2B50;</span> Why Details Networks</a>
                <a href="#reviews" class="hero-icon-link"><span class="me-1">&#x1F31F;</span> Customer Reviews</a>
                <a href="#connect" class="hero-icon-link"><span class="me-1">&#x2709;&#xFE0F;</span> Let's Connect</a>
            </div>
            <img src="/images/موقع11.png" alt="Network cables" class="hero-img mt-3" />

        </div>
    </section>

